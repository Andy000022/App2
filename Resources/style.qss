/*= Estilo para los icon only Inicia 
  ===============================================*/

    #icon_only_widget{
        background-color:#313a46;
        width:50px;
    }

    /*Estilo para el Qpushbuutoon y el Qlabel Inicia*/
    #icon_only_widget QPushButton, QLabel{
        height:50px;
        border:none; /* Aqu√≠ estaba el error */
        /* border-bottom: 1px solid #b0b0b0*/
    }

    #icon_only_widget QPushButton:hover{
        background-color: rgba(86, 101, 115, 0.5);
    }

    /* estilo para el logo*/
    #logo_label_1{
        padding:5px;
    }
/*= Fin 
===============================================*/

/*= Estilo para los botones en el menu Inicia 
  ===============================================*/

    #full_menu_widget{
        background-color:#313a46;
    }

    #full_menu_widget QPushButton{
        border:none;
        border-radius: 3px;
        text-align: left;
        padding: 8px 0 8px 15px;
        color: #788596;
    }

    #full_menu_widget QPushButton:hover{
        background-color: rgba(86, 101, 115, 0.5);
    }

    #full_menu_widget QPushButton:checked{
        color:#fff;
    }

    /* estilo para el logo image*/
    #logo_label_2{
        padding:5px;
        color:#fff;
    }

    /* estilo para app titulo*/
    #logo_label_3{
        padding-right:10px;
        color:#fff;
    }
/*= Fin 
===============================================*/

